venv/
.env/
*.env
*.venv
*.env.*

# Ignore logs and temporary files
logs/
*.log
*.tmp
*.bak

# Ignore IDE-specific files
.idea/
.vscode/

# Ignore test files
tests/
*.pytest_cache/
*.cover
.coverage
nosetests.xml
*.test

